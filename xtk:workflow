            <set value="linkedData" xpath="/ignored/@splitType"/>
          </if>
          <if expr="NodeValue('extension/schema[@name=\''+ExtractRight(@schema,':')+'\']/element/@dataSource')!=''">
            <set value="extSharedData" xpath="/ignored/@splitType"/>
