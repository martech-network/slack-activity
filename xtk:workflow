                    <leave>
                      <check expr="[/tmp/dbImportAdd/@tableName] != ''">
                        <error>You must specify the table name.</error>
                      </check>
                      <check expr="[/tmp/dbImportAdd/@tableLabel] != ''">
                        <error>You must specify the table label.</error>
                      </check>
                    </leave>
