                      <static label="If necessary, you can keep the sequence of records selected by the deduplication process."
                              type="help"/>
                      <input xpath="@keepRank"/>
                    </container>
                  </form>
                </input>
              </container>
              <leave>
            <!-- to build display -->
                <set value="1" xpath="/ignored/@display"/>
