                  </enter>
                  <container maxLabelWidth="200" type="visibleGroup" visibleIf="[/tmp/queryAdd/@calcOffer]='dynamic'"
                             xpath="/tmp/queryAdd">
                    <input computeStringAlias="@offerSpace-cs" createMode="none"
