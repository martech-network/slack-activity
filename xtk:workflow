  <!--[of]:  schemaSelector-->
      <container name="schemaSelector">
        <container type="visibleGroup" visibleIf="[/tmp/@acceptTempSchema]==1">
          <input checkedValue="0" label="Database schema" type="RadioButton" xpath="/tmp/@useTempSchema"/>
